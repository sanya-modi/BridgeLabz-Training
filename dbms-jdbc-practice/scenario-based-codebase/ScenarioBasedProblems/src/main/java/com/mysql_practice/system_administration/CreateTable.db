/* */

CREATE TABLE specialties_dc (
    specialty_id INT PRIMARY KEY AUTO_INCREMENT,
    specialty_name VARCHAR(100) UNIQUE NOT NULL
);
+----------------+--------------+------+-----+---------+----------------+
| Field          | Type         | Null | Key | Default | Extra          |
+----------------+--------------+------+-----+---------+----------------+
| specialty_id   | int          | NO   | PRI | NULL    | auto_increment |
| specialty_name | varchar(100) | NO   | UNI | NULL    |                |
+----------------+--------------+------+-----+---------+----------------+

CREATE TABLE doctors_dc (
    doctor_id INT PRIMARY KEY AUTO_INCREMENT,
    doctor_name VARCHAR(100) NOT NULL,
    specialization VARCHAR(100),
    max_daily_capacity INT DEFAULT 10,
    specialty_id INT,
    FOREIGN KEY (specialty_id) REFERENCES specialties_dc(specialty_id)
);
+--------------------+--------------+------+-----+---------+----------------+
| Field              | Type         | Null | Key | Default | Extra          |
+--------------------+--------------+------+-----+---------+----------------+
| doctor_id          | int          | NO   | PRI | NULL    | auto_increment |
| doctor_name        | varchar(100) | NO   |     | NULL    |                |
| specialization     | varchar(100) | YES  |     | NULL    |                |
| max_daily_capacity | int          | YES  |     | 10      |                |
| specialty_id       | int          | YES  | MUL | NULL    |                |
+--------------------+--------------+------+-----+---------+----------------+